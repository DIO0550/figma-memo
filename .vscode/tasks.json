{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Docusaurus: Start Development Server",
      "type": "shell",
      "command": "yarn",
      "args": ["start"],
      "group": "build",
      "isBackground": true,
      "problemMatcher": [],
      "options": {
        "cwd": "${workspaceFolder}/website"
      }
    },
    {
      "label": "Docusaurus: Build for Production",
      "type": "shell",
      "command": "yarn",
      "args": ["build"],
      "group": "build",
      "problemMatcher": [],
      "options": {
        "cwd": "${workspaceFolder}/website"
      }
    },
    {
      "label": "Docusaurus: Serve Built Site",
      "type": "shell",
      "command": "yarn",
      "args": ["serve"],
      "group": "test",
      "isBackground": true,
      "problemMatcher": [],
      "options": {
        "cwd": "${workspaceFolder}/website"
      }
    },
    {
      "label": "Docusaurus: Clear Cache",
      "type": "shell",
      "command": "yarn",
      "args": ["clear"],
      "group": "build",
      "problemMatcher": [],
      "options": {
        "cwd": "${workspaceFolder}/website"
      }
    },
    {
      "label": "Install Dependencies",
      "type": "shell",
      "command": "yarn",
      "args": ["install"],
      "group": "build",
      "problemMatcher": [],
      "options": {
        "cwd": "${workspaceFolder}/website"
      }
    }
  ]
}
